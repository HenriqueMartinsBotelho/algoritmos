class Dummy_Class:

	def dummy_function(self):
		print("this is a dummy function")

	def dummy_variable_receiver(self, x):
		print("Your dummy variable is: " + str(x))

	def number_odds_verifier(self, y):
		return y%2==0

class Modularizacao_De_Entrada:
	
	def PP(x: int)->int:
		return QQ(x) + QQ(x+1)
	
	def QQ(y: int)-> int:
		return y*2

	print(PP(int(input)))

if "__name__" == "__main__":
	init_class_test = Dummy_Class()
	verifier = dummy_function()
	if isinstance(verifier, None):
		print("Wow, that's really a dummy function")
	else:
		dummy_viriable_receiver(5)

	for i in range(100):
		if isinstance(number_odds_verifier(i), True):
			print("The number " + str(i) + " is even")
		else:
			print("The number " + str(i) + " is odd")
	print("Teste modular...\n")
	Modularizacao_De_Entrada()
